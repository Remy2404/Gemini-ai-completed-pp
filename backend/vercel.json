{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "/index.js"
    },
    {
      "src": "/api/chats",
      "dest": "/index.js"
    },
    {
      "src": "/api/userchats",
      "dest": "/index.js"
    },
    {
      "src": "/api/chats/:id",
      "dest": "/index.js"
    },
    {
      "src": "/api/chats/:id",
      "methods": ["PUT"],
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/dist/$1"
    }
  ],
  "env": {
    "PORT": "3000",
